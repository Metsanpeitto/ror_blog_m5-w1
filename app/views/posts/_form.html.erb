<!--<h1><%= t('.sign_up') %></h1>-->
<div class="row">
  <div class="col-9">
    <%= form_for @post, remote: true do |form| %>
      <h3 class="title"> Title </h3>
      <%= form.text_field :title %>
      <h3 class="text"> Post text </h3>
      <%= form.text_area :text, class: 'text-area-form' %>
      <%= form.submit "Post" , data: {disable_with: "Loading..."}%>
    <% end %>
  </div>
</div>
